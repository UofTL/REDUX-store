{
    "name": "redux-store",
    "version": "1.0.0",
    "description": "we are building online shop with REDUX payement",
    "main": "index.js",
    "scripts": {
        "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
        "start:prod": "cd server && npm start",
        "start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
        "install": "cd server && npm i && cd ../client && npm i",
        "seed": "cd server && npm run seed",
        "heroku-postbuild": "cd client && npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UofTL/REDUX-store.git"
    },
    "keywords": [
        "REDUX",
        "MERN",
        "Stack",
        "Full",
        "Stack",
        "Online",
        "Store",
        "with",
        "payement",
        "REACT"
    ],
    "author": "Eleonore Caclard",
    "license": "MIT",
    "dependencies": {
        "if-env": "^1.0.4"
    },
    "devDependencies": {
        "concurrently": "^5.1.0"
    },
    "bugs": {
        "url": "https://github.com/UofTL/REDUX-store/issues"
    },
    "homepage": "https://github.com/UofTL/REDUX-store#readme"
}
